{
  "name": "@easy-editor/materials-dashboard-gauge-chart",
  "version": "0.0.6",
  "description": "Gauge Chart component for EasyEditor dashboard",
  "type": "module",
  "main": "src/index.tsx",
  "types": "src/index.tsx",
  "exports": {
    ".": "./src/index.tsx"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.min.js",
    "jsdelivr": "dist/index.min.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/Easy-Editor/EasyMaterials",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "dashboard",
    "gauge-chart",
    "echarts",
    "component",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyMaterials",
    "directory": "packages/dashboard/chart/gauge-chart"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyMaterials/issues"
  },
  "scripts": {
    "dev": "easypack dev",
    "dev:debug": "vite --port 5006",
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "easypack build"
  },
  "peerDependencies": {
    "@easy-editor/core": "*",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@easy-editor/materials-shared": "workspace:*",
    "echarts": "^5.6.0"
  }
