    "@babel/preset-typescript": "^7.26.0",
    "@biomejs/biome": "2.3.8",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@easy-editor/easypack": "^0.0.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
